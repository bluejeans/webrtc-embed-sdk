      "react-dom": "16.8.0",
      "styled-components": "5.3.11"
    },
    "devDependencies": {
      "@types/react": "^18.2.8",
      "@types/react-dom": "^18.2.4",
      "html-webpack-plugin": "^5.5.1",
      "pre-commit": "^1.2.2",
      "prettier": "^2.8.8",
      "source-map-loader": "4.0.1",
      "ts-loader": "9.4.3",
      "typescript": "^5.1.3",
      "typescript-plugin-styled-components": "^2.0.0",
      "webpack": "5.85.0",
      "webpack-cli": "^5.1.1",
