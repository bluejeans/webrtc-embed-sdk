{
    "name": "@bluejeans/bjn-meeting-experience",
    "version": "2.2.0",
    "description": "BlueJeans JS Meeting client - WebRTC",
    "homepage": "https://bluejeans.com",
    "scripts": {
      "format": "prettier --write .",
      "check-format": "prettier --check .",
      "precommit-msg": "echo running precommit checks ... ",
      "build": "webpack --config webpack.config.js --mode=development",
      "sample": "webpack-dev-server --config webpack.config.js --mode=development"
